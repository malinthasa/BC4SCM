{
    "name": "network-supplier",
    "version": "1.0.0",
    "client": {
        "organization": "Supplier",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "Supplier": {
            "mspid": "SupplierMSP",
            "peers": [
                "peer0.supplier.bc4scm.de",
                "peer1.supplier.bc4scm.de"
            ]
        }
    },
    "peers": {
        "peer0.supplier.bc4scm.de": {
            "url": "grpcs://localhost:11051",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/supplier.bc4scm.de/tlsca/tlsca.supplier.bc4scm.de-cert.pem"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.supplier.bc4scm.de"
            }
        },
        "peer1.supplier.bc4scm.de": {
            "url": "grpcs://localhost:12051",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/supplier.bc4scm.de/tlsca/tlsca.supplier.bc4scm.de-cert.pem"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer1.supplier.bc4scm.de"
            }
        }
    }
}
